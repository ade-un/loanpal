{
    "version": 2,
    "builds": [
        {
            "src": "loan_management/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1" 
        },
        {
            "src": "/(.*)",
            "dest": "loan_management/wsgi.py"
        }
    ]
}
